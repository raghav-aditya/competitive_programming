A.parrellstream().forEach( x -> {
		if( x == 2 )
		log.info("green");
		else
		log.info("red");
});


void green()
{
	
}

void red(){
	
}

Thread t1 = new Thread();
Thread t2 = new Thread();

for( int i = 0 ; i < N ; i++ )
{
	if(A[i]==2)
	{
	  t1.execute(()->{green()});
	}
	else
	t2.execute(()->{red()});
}




void synchronised green_red( int i )
{
	if( A[i] == 2 )
	green();

	else
	red();
}




