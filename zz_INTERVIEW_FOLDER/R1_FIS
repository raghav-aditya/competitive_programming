// 

@RestController
public CarDealerController
{

	@Autowired
	private DealerShipService Dss ;
	

	@GetMapping( "vehical/color")
	public ApiResponse getVehical( @RequestHeader String token ,  @RequestParamer String color )
	{

		try
		{

		OauthService.autherise(token);



		List< Vehical > vehicals = Dss.getVehical( color );

		ApiResponse apiResponse = new ApiResponse ;

		apiResponse.setBody(vehical);


		return apiResponse ;


		} catch( OauthException ex)
		{
			log.error(" token is not validated or expired" );
			throw ex ;
		}



	}

	
}


